{
    // Format settings
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.fixAll.eslint": false
    },
    "eslint.format.enable": true,
    "eslint.alwaysShowStatus": true,
    "prettier.eslintIntegration": true
}